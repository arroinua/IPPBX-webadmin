            "groupno":          "string",
            
            /* "kind": "icd" */
            "groupno":          "string",
            "autologin":        "boolean",
            "priority":         "integer",
            "maxlines":         "integer",
            "canpickup":        "boolean",
            "method":           "integer",
            "natimeout":        "integer",
            "resumetime":       "integer",
            "greeting":         "string",
