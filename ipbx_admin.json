            "monitordeny":      "boolean",
            "callwaiting":      "boolean",
            "outcallbarring":   "boolean",
            "costroutebarring": "boolean",
            "recording":        "boolean",
            "voicemail":        "boolean",
            "bnumbertransforms": [
