            "busyoverdeny": "boolean" ,
            "monitordeny":  "boolean",
            "callwaiting":  "boolean",
            "huntbusy":     "boolean",
            "huntnans":     "boolean",
            "huntnreg":     "boolean",
            "voicemail":    "boolean",
            "fwdallnumber":     "string",
            "fwdbusynumber":    "string",
            "fwdnansnumber":    "string",
