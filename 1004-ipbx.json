/* 
 * Изменение состояния объекта 
 * 
 * method:  "stateChanged"
 * 
 * params:  
 *          "oid":      идентификатор объекта
 *          "ext":      внутренний номер телефона
 *          "state":    состояние
 *                      0 - свободен
 *                      1 - исходящий вызов
 *                      2 - входящий вызов
 *                      3 - соединен
 *                      4 - включена функция "не беспокоить"
 *                      5 - не зарегистрирован или отключен
 *                      6 - установлена переадресация на внешний номер
 *                     -1 - удален
 *          
 *          "name":     имя
 *          "group":    имя группы
 *          "reg":      ip адрес или номер переадресации
 * 
 */

"stateChanged": {
    "type":             "indication",
    
    "params": {
        "oid":          "string",
        "ext":          "string",
        "state":        "number",
        "name":         "string",
        "group":        "string",
        "reg":          "string"
    }
},


